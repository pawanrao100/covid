<!DOCTYPE html>
<html lang="en">

<head>
    <title>
        <%= title %>
    </title>
    <link href="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <!------ Include the above in your HEAD tag ---------->
    
    <script language="JavaScript" src="https://code.jquery.com/jquery-1.11.1.min.js" type="text/javascript"></script>
    <script language="JavaScript" src="https://cdn.datatables.net/1.10.4/js/jquery.dataTables.min.js" type="text/javascript"></script>
    <script language="JavaScript" src="https://cdn.datatables.net/plug-ins/3cfcc339e89/integration/bootstrap/3/dataTables.bootstrap.js" type="text/javascript"></script>
    <link rel="stylesheet" type="text/css" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
    
    <link rel="stylesheet" type="text/css" href="http://cdn.datatables.net/plug-ins/3cfcc339e89/integration/bootstrap/3/dataTables.bootstrap.css">

    <link href="/css/style.css" rel="stylesheet" type="text/css">
</head>

<body>
    <h1 class="text-center"><%= title %></h1>
    <div class="container row">
	<div class='sidenav col-3' id='navBar'>
	    <div class='list-group'>
	        <div href='/' class='list-group-item active list-group-item-action' data-toggle="list" role="tab">Home</div>
	        <div href='/state' class='list-group-item list-group-item-action' data-toggle="list" role="tab">State Data</div>
	        <div href='#' class='list-group-item list-group-item-action' data-toggle="list" role="tab">District Data</div>
	        <div href='#' class='list-group-item list-group-item-action' data-toggle="list" role="tab">In-Progress</div>
	        <div href='#' class='list-group-item list-group-item-action' data-toggle="list" role="tab">Completed</div>
	    </div>
	</div>
	<div class='col-9' id='main'>
		<table id="datatable" class="table table-striped table-bordered" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>active</th>
                    <th>deaths</th>
                    <th>confirmed</th>
                    <th>newdeaths</th>
                    <th>recovered</th>
                    <th>newconfirmed</th>
                    <th>newrecovered</th>
                    <th>migratedother</th>
                </tr>
            </thead>

           
            <tbody>
                
                <%  results.forEach(function(data){ %>

                
                <tr>
                    <td><a href="/districtData/<%= data.code %>"><%= data.name %></a></td>
                    <td><%= data.active %></td>
                    <td><%= data.deaths %></td>
                    <td><%= data.confirmed %></td>
                    <td><%= data.newdeaths %></td>
                    <td><%= data.recovered %></td>
                    <td><%= data.newconfirmed %></td>
                    <td><%= data.newrecovered %></td>
                    <td><%= data.migratedother %></td>
                </tr>
                <% }); %>
            </tbody>
        </table> 
	</div>
</div>
<script>
$(document).ready(function() {
    $('#datatable').dataTable();
    
     $("[data-toggle=tooltip]").tooltip();
    
} );
</script>
</body>

</html>